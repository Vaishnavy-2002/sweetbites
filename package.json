{
  "name": "sweetbite-frontend",
  "version": "1.0.0",
  "description": "SweetBite - Online Cake Delivery with Customization",
  "private": true,
  "proxy": "http://localhost:8000",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@react-spring/three": "^9.7.5",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.4.0",
    "framer-motion": "^10.12.16",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "mapbox-gl": "^2.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.1.9",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "three": "^0.164.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && python manage.py runserver",
    "frontend": "react-scripts start",
    "install-all": "npm install && cd backend && pip install -r requirements.txt",
    "migrate": "cd backend && python manage.py migrate",
    "superuser": "cd backend && python manage.py createsuperuser",
    "setup": "npm run install-all && npm run migrate",
    "railway:deploy": "railway up",
    "railway:logs": "railway logs",
    "railway:shell": "railway shell"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2"
  }
}